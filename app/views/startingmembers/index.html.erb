<h1>Listing startingmembers</h1>

<table>
  <tr>
    <th>Game date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @startingmembers.each do |startingmember| %>
  <tr>
    <td><%= startingmember.game_date %></td>
    <td><%= link_to 'Show', startingmember %></td>
    <td><%= link_to 'Edit', edit_startingmember_path(startingmember) %></td>
    <td><%= link_to 'Destroy', startingmember, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Startingmember', new_startingmember_path %>
